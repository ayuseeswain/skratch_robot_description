<?xml version="1.0"?>
<package format="2">
  <name>kinova_arm_moveit_config</name>
  <version>0.3.0</version>
  <description>
    Automatically generated package with configuration and launch files for using the Kinova arm with MoveIt
  </description>

  <maintainer email="chaitanyagumudala961@gmail.com">Chaitanya Gumudala</maintainer>
  <license>TODO: License declaration</license>

  <url type="website">http://moveit.ros.org/</url>
  <url type="bugtracker">https://github.com/ros-planning/moveit/issues</url>
  <url type="repository">https://github.com/ros-planning/moveit</url>

  <author email="chaitanyagumudala961@gmail.com">Chaitanya Gumudala</author>

